<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.livroandroid.hellotoolbar">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="br.com.livroandroid.hellotoolbar.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.livroandroid.hellotoolbar.ExemploToolbar1Activity"
            android:parentActivityName="br.com.livroandroid.hellotoolbar.MainActivity" android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="br.com.livroandroid.hellotoolbar.ExemploToolbar2Activity"
            android:parentActivityName="br.com.livroandroid.hellotoolbar.MainActivity" />
    </application>

</manifest>
